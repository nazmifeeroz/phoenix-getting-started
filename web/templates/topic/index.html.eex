<h5>Topics</h5>

<ul class="collection">
  <%= for topic <- @topics do %>
    <li class="collection-item">
      <%= topic.title %>
    </li>
  <% end %>
</ul>

<div class="fixed-action-btn">
  <%= link to: topic_path(@conn, :new), class: "waves-effect waves-circle waves-light btn-floating btn-large red" do %>
    <i class="material-icons">add</i>
  <% end %>
</div>